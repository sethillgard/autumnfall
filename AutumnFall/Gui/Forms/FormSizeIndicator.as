package AutumnFall.Gui.Forms 
{
	import AutumnFall.Containers.UniqueContainer;
	
	/**
	 * Use form size indicators to set the default size of a form.
	 */
	public class FormSizeIndicator extends UniqueContainer
	{
		public function FormSizeIndicator() 
		{
			//Use alpha so width and height are not affected.
			this.alpha = 0;
		}
		
	}
	
}